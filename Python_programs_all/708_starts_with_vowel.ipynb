    "import re \n",
    "\n",
    "def startswithvowel(s):\n",
    "    pattern = '^[aeiouAEIOU].*'\n",
    "    if re.match(pattern,s):\n",
    "        return(\"accepted\")\n",
    "    else:\n",
    "        return(\"not accepted\")\n",
    "\n",
    "s1 = 'zebra'\n",
    "s2 = 'Alaska'\n",
    "print(startswithvowel(s1))\n",
    "print(startswithvowel(s2))"
   ]
  }
