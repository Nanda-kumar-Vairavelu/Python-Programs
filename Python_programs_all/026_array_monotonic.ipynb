    "def ismonotonic(a):\n",
    "    return (all(a[i] >= a[i+1] for i in range(len(a)-1)) or\n",
    "            all(a[i] <= a[i+1] for i in range(len(a)-1)))\n",
    "\n",
    "array = [5,6,7,8]\n",
    "print (ismonotonic(array))"
   ]
