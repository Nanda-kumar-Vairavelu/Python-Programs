    "def has_spec_char(s):\n",
    "    for c in s:\n",
    "        if not(c.isalpha() or c.isdigit() or c==' '):\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "s = \"Hello#world\"\n",
    "if has_spec_char(s):\n",
    "    print(\"has special chars\")"
