    "import numpy as np \n",
    "\n",
    "def nthlargest(a,N):\n",
    "    a =  np.array(a)\n",
    "    return a[np.argsort(a)[-N]]\n",
    "\n",
    "a = [1,2,3,4,5,6]\n",
    "N = 3\n",
    "print(nthlargest(a,N))"
   ]
  }
