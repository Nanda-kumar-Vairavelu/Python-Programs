    "def sum_of_squares(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n**2 + sum_of_squares(n-1)\n",
    "\n",
    "num = 4\n",
    "sum_of_squares = sum_of_squares(num)\n",
    "print(sum_of_squares)"
