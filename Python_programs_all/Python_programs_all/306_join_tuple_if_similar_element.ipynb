   "source": [
    "import pandas as pd\n",
    "\n",
    "test_list = [(5,6),(5,7),(5,8),(1,2),(1,3),(1,4)]\n",
    "\n",
    "df = pd.DataFrame(test_list, columns=[\"A\",\"B\"])\n",
    "\n",
    "grouped = df.groupby(\"A\")[\"B\"].apply(list)\n",
    "\n",
    "res = [tuple([k]+v) for k,v in grouped.items()]\n",
    "\n",
    "print(res)"
   ]
  }
