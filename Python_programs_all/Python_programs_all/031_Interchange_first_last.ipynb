    "def interchange(newlist1):\n",
    "    size = len(newlist1)\n",
    "\n",
    "    temp = newlist1[0]\n",
    "    newlist1[0] = newlist1[size -1]\n",
    "    newlist1[size -1] = temp\n",
    "\n",
    "    return newlist1\n",
    "\n",
    "newList = [1,6,3,7,8]\n",
    "print(interchange(newList))"
   ]
  }
