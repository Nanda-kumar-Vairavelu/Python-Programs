    "def largest_prime_factor(n):\n",
    "    i = 2\n",
    "    largest_prime = -1\n",
    "\n",
    "    while i**2 <= n:\n",
    "        while n%i==0:\n",
    "            largest_prime = i\n",
    "            n = n//i\n",
    "        i = i + 1\n",
    "    if n > i:\n",
    "        largest_prime = n\n",
    "        return largest_prime\n",
    "\n",
    "n = 15\n",
    "print(largest_prime_factor(n))"
   ]
  }
